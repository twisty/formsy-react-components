{
  "name": "formsy-react-components",
  "version": "2.0.0-alpha.2",
  "description": "Bootstrap components for a formsy-react form.",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:full": "jest --coverage --verbose",
    "lint": "eslint src examples --ext .js,.jsx,.ts,.tsx",
    "build": "tsc",
    "prepublishOnly": "./scripts/prepublish.sh",
    "dev": "tsc --watch",
    "examples": "webpack-dev-server --config examples/webpack.config.js",
    "examples:build": "webpack -p --config examples/webpack.config.js",
    "gh-pages": "NODE_ENV=production yarn examples:build && gh-pages -d examples",
    "generate-docs": "./scripts/generate-docs.sh"
  },
  "main": "./release/index.js",
  "types": "./release/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/twisty/formsy-react-components.git"
  },
  "files": [
    "/release"
  ],
  "keywords": [
    "react",
    "formsy-react",
    "bootstrap",
    "react-component"
  ],
  "author": "Tim Brayshaw <tim@brayshaw.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twisty/formsy-react-components/issues"
  },
  "peerDependencies": {
    "formsy-react": "^2.0.0-beta.2",
    "react": "^16.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.3",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.13.0",
    "formsy-react": "^2.0.0-beta.2",
    "gh-pages": "^2.0.0",
    "jest": "^24.1.0",
    "prettier": "^1.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.0",
    "react-docgen-typescript": "^1.12.5",
    "react-docs-markdown": "^0.8.0",
    "react-dom": "^16.3.0",
    "react-test-renderer": "^16.3.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.9"
  }
}
